<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--一个namespace对应一个接口-->
<mapper namespace="dao.DiagnoseMapper">
    <insert id="insertDiagnose">
        insert into diagnose(id, regId, diseaseName) values (0, #{regId}, #{diseaseName});
    </insert>
</mapper>